{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "web-app",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "web-app": {
      "projectType": "application",
      "root": "apps/web-app",
      "sourceRoot": "apps/web-app/src",
      "prefix": "msk",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/web-app",
            "index": "apps/web-app/src/index.html",
            "main": "apps/web-app/src/main.ts",
            "polyfills": "apps/web-app/src/polyfills.ts",
            "tsConfig": "apps/web-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/web-app/src/favicon.ico",
              "apps/web-app/src/assets"
            ],
            "styles": [
              "apps/web-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/web-app/src/environments/environment.ts",
                  "with": "apps/web-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "web-app:build:production"
            },
            "development": {
              "browserTarget": "web-app:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/web-app/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/web-app/src/**/*.ts",
              "apps/web-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/web-app"
          ],
          "options": {
            "jestConfig": "apps/web-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "web-app-e2e": {
      "root": "apps/web-app-e2e",
      "sourceRoot": "apps/web-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/web-app-e2e/cypress.json",
            "tsConfig": "apps/web-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "web-app:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "web-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/web-app-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "web-api": {
      "root": "apps/web-api",
      "sourceRoot": "apps/web-api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/web-api",
            "main": "apps/web-api/src/main.ts",
            "tsConfig": "apps/web-api/tsconfig.app.json",
            "assets": [
              "apps/web-api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/web-api/src/environments/environment.ts",
                  "with": "apps/web-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "web-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/web-api/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/web-api"
          ],
          "options": {
            "jestConfig": "apps/web-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shell-feature": {
      "projectType": "library",
      "root": "libs/shell/feature",
      "sourceRoot": "libs/shell/feature/src",
      "prefix": "msk",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shell/feature"
          ],
          "options": {
            "jestConfig": "libs/shell/feature/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shell/feature/src/**/*.ts",
              "libs/shell/feature/src/**/*.html"
            ]
          }
        }
      }
    },
    "shell-ui-layout": {
      "projectType": "library",
      "root": "libs/shell/ui/layout",
      "sourceRoot": "libs/shell/ui/layout/src",
      "prefix": "msk",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shell/ui/layout"
          ],
          "options": {
            "jestConfig": "libs/shell/ui/layout/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shell/ui/layout/src/**/*.ts",
              "libs/shell/ui/layout/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-app-config": {
      "projectType": "library",
      "root": "libs/shared/app-config",
      "sourceRoot": "libs/shared/app-config/src",
      "prefix": "msk",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/app-config"
          ],
          "options": {
            "jestConfig": "libs/shared/app-config/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/app-config/src/**/*.ts",
              "libs/shared/app-config/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-utils": {
      "projectType": "library",
      "root": "libs/shared/utils",
      "sourceRoot": "libs/shared/utils/src",
      "prefix": "msk",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/utils"
          ],
          "options": {
            "jestConfig": "libs/shared/utils/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/utils/src/**/*.ts",
              "libs/shared/utils/src/**/*.html"
            ]
          }
        }
      }
    }
  }
}
