{
  "name": "client-shared-ui-highlight",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/client/shared/ui/highlight/src",
  "prefix": "msk",
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/client/shared/ui/highlight/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/client/shared/ui/highlight/**/*.ts",
          "libs/client/shared/ui/highlight/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:client-shared", "type:ui"]
}
